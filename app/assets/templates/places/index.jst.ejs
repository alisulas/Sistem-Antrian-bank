<!-- <div class="map"></div> -->

<div class='places-index-title'>
  <!-- <h3>Places</h3> -->
</div>

<ul class='places'>
  <% places.each(function (place) { %>
      <a href="#/places/<%= place.id %>" data-place-id="<%= place.id %>">
    <li>
        <span class='details'>
          <strong><%= place.escape("title") %></strong>
        </span>
        <div class='place-description'>
          <%= place.escape('description') %>
        </div>
    </li>
      </a>
  <% }); %>
</ul>
