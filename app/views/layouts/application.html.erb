<!DOCTYPE html>
<html>
<head>
  <title>WaiterUp</title>

  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>

  <script type="text/javascript"
  src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD7vYTae_GpOuNdjgPs1JkKzMFVBIHs7EE">
  </script>
</head>
<body class="login-body">
  <%= render 'layouts/header' %>
  <div class="container">
    <div class="row">
      <div>
        <% if flash[:notice] %>
        <p><%= flash[:notice] %></p>
        <% end %>

        <% if flash[:errors] %>
        <ul>
          <% flash[:errors].each do |err| %>
          <li><%= err %></li>
          <% end %>
        </ul>
        <% end %>

        <%= yield %>
      </div>
    </div>
    <!-- <%#= render 'layouts/footer' %> -->
  </div>
</body>
</html>
